<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yhy.widgetdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AdvActivity"/>
        <activity android:name=".TitleActivity"/>
        <activity android:name=".RvActivity">
        </activity>
        <activity android:name=".ETVActivity">
        </activity>
        <activity android:name=".RIVActivity">
        </activity>
        <activity android:name=".LoadingActivity">
        </activity>
        <activity android:name=".SwitchButtonActivity">
        </activity>
        <activity android:name=".SettingsActivity">
        </activity>
    </application>

</manifest>